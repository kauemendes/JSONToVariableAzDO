{
    "manifestVersion": 1,
    "id": "build-release-task",
    "name": "KaueCode JSON to Variables",
    "version": "1.0.4",
    "publisher": "KaueFreitasCode",
    "description": "Tool for converting JSON files to Azure DevOps environment variables",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "buildandreleasetask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildandreleasetask"
            }
        }
    ],
    "content": {
		"details": {
		    "path": "overview.md"
		},
		"license": {
		    "path": "LICENSE"
		}
  	}
}